{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Wing Player",
			"type": "chrome",
			"request": "launch",
			"file": "index.html",
			"runtimeExecutable": "${execPath}",
			"useBuildInServer": true,
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}",
			"preLaunchTask":"build",
			"port":5246,
			"runtimeArgs": [
				"--allow-file-access-from-files",
				"--allow-file-access-frome-files",
				" --disable-web-security"
			]
		},
		{
			"name": "Launch Chrome",
			"type": "chrome",
			"request": "launch",
			"file": "index.html",
			"useBuildInServer": true,
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}",
			"preLaunchTask":"build",
			"userDataDir":"${tmpdir}",
			"port":15246
			// ,
			// "runtimeArgs": [
			// 	"--allow-file-access-from-files",
			// 	"--allow-file-access-frome-files",
			// 	" --disable-web-security"
			// ]
		}
	]
}